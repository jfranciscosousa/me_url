{
  "name": "me_url",
  "version": 2,
  "alias": ["urls.wtf"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    },
    { "src": "api/index.js", "use": "@now/node-server" }
  ],
  "routes": [
    {
      "src": "/new_url",
      "dest": "/api/index.js"
    },
    {
      "src": "/u/(?<hash>[^/]+)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "FAUNADB_SECRET": "@faunadb_secret",
    "NODE_ENV": "production"
  }
}
