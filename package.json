{
  "name": "urls.wtf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf .svelte-kit .vercel node_modules",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "ts-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "ts-check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "ci": "npm run lint && npm run ts-check && npm run build"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.3.2",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.40.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "svelte": "5.0.0-next.164",
    "svelte-check": "^3.8.1",
    "svelte-preprocess": "^6.0.1",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "vite": "5.3.1"
  },
  "type": "module",
  "dependencies": {
    "fauna": "^2.0.1"
  }
}
