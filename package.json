{
  "name": "urls.wtf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf .svelte-kit .vercel node_modules",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "ts-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "ts-check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "ci": "npm run lint && npm run ts-check && npm run build"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-vercel": "^2.1.1",
    "@sveltejs/kit": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.4",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "type": "module"
}
