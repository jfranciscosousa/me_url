{
  "name": "urls.wtf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf .svelte-kit .vercel node_modules",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "ts-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "ts-check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "ci": "npm run lint && npm run ts-check && npm run build"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.25.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.30",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "4.5.0"
  },
  "type": "module",
  "dependencies": {
    "fauna": "^1.3.0"
  }
}
