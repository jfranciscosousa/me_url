{
  "name": "urls.wtf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf .svelte-kit .vercel node_modules",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "ts-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "ts-check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "ci": "npm run lint && npm run ts-check && npm run build"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^3.0.2",
    "@sveltejs/kit": "^1.22.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.27",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "4.4.7"
  },
  "type": "module"
}
