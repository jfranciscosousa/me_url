{
  "name": "urls.wtf",
  "description": "minimalistic url shortener",
  "version": "1.0.0",
  "author": "Francisco Sousa",
  "homepage": "https://github.com/jfranciscosousa/urls.wtf",
  "keywords": [
    "url",
    "shortener"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jfranciscosousa/urls.wtf"
  },
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "dev": "netlify dev -c 'pnpm run eleventy --serve'",
    "prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "build": "NODE_ENV=production eleventy",
    "lint": "eslint src",
    "lint-styles": "stylelint \"src/**/*.css\" \"src/**/*.scss\" \"src/**/*.js\""
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "graphql": "^15.3.0",
    "graphql-request": "^3.0.0",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "normalize.css": "^8.0.1",
    "postcss": "^7.0.32",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "precss": "^4.0.0",
    "secure-random-string": "^1.1.3",
    "terser": "^5.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "netlify-cli": "^2.61.2",
    "prettier": "^2.1.1",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-order": "^4.1.0"
  }
}
